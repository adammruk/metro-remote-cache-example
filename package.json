{
  "name": "MetroRemoteCache",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "CACHE_READ_ONLY=true react-native run-android",
    "ios": "CACHE_READ_ONLY=true react-native run-ios",
    "lint": "eslint .",
    "start": "CACHE_READ_ONLY=true react-native start",
    "test": "jest",
    "build": "react-native bundle --verbose --platform ios --dev true --entry-file index.js --bundle-output dist/ios/main.jsbundle --assets-dest dist/ios --config metro.config.js --minify false --reset-cache"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.804.0",
    "react": "19.0.0",
    "react-native": "0.78.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
